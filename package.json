{
  "name": "first-web-game-maker",
  "displayName": "First Web Game Maker",
  "description": "First Web Game Maker",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "first-web-game-maker.paperFortune",
        "title": "おみくじ"
      },
      {
        "command": "first-web-game-maker.insertHelloWorldAtTop",
        "title": "insert hello world at top"
      },
      {
        "command": "first-web-game-maker.insertTextAtCursor",
        "title": "insert text at cursor"
      },
      {
        "command": "first-web-game-maker.deleteAfterCursor",
        "title": "delete after cursor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "2.8.4",
    "typescript": "^4.9.5"
  }
}
